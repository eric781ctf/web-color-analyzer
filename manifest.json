{
  "manifest_version": 3,
  "name": "Pixel Color Picker",
  "version": "2.1",
  "description": "Pick screen pixel color, analyze contrast, and preview HEX/RGB with pins.",
  "permissions": ["tabs", "activeTab", "clipboardWrite"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
